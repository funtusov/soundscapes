{
  "name": "etherpad-soundscape",
  "version": "1.0.0",
  "description": "An interactive audio-visual synthesizer instrument for touch devices.",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "npm run build && netlify deploy --prod --dir=dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "ios:init": "npx cap add ios",
    "ios:sync": "npm run build && npx cap sync ios",
    "ios:open": "npx cap open ios",
    "ios:dev": "npx cap run ios"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/funtusov/soundscapes.git"
  },
  "keywords": [
    "synthesizer",
    "web-audio",
    "instrument",
    "multitouch"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/funtusov/soundscapes/issues"
  },
  "homepage": "https://etherpad-soundscape.netlify.app",
  "devDependencies": {
    "typescript": "^5.9.3",
    "vite": "^7.2.7",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "@capacitor/cli": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@capacitor/ios": "^8.0.0",
    "@mediapipe/tasks-vision": "^0.10.22-rc.20250304",
    "ios-haptics": "^0.1.4"
  }
}
